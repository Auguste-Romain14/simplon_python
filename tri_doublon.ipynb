   "source": [
    "# Ce programme est une fonction qui fait le tri de doublon dans une liste\n",
    "\n",
    "initiale = [1, 1, 2, 4, 9, 2, 5, 4]\n",
    "\n",
    "def del_doubl(liste):\n",
    "    tri = []\n",
    "    for i in liste:\n",
    "        if i not in tri:\n",
    "            tri.append(i)\n",
    "    \n",
    "    return tri\n",
    "\n",
    "print(del_doubl(initiale))"
   ]
  }
