   "source": [
    "# ce programme est une fonction qui prend un parametre N et renvoie les nombres impairs entre 0 et N\n",
    "\n",
    "def nb_impairs(N):\n",
    "    liste = []\n",
    "    i = 0\n",
    "    while i <= N:\n",
    "        if not (i % 2 == 0):\n",
    "            liste.append(i)\n",
    "        i += 1\n",
    "\n",
    "    return liste"
   ]
  }
